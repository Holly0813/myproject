    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import xlrd\n",
    "import openpyxl \n",
    "\n",
    "#原excel表格所在位置\n",
    "file_path = r'E:/我的又一个桌面/大创/项目内容/北邮一万条数据.XLSX'\n",
    "\n",
    "\n",
    "#读取Excel表格，默认读取第一个工作簿，输出表格信息\n",
    "df = pd.read_excel(file_path)\n",
    "height, width = df.shape\n",
    "print(height, width, type(df))\n",
    "\n",
    "# 统计每年公开的专利数目\n",
    "\n",
    "t_2013 = len(df[df['公开(公告)日'].str.contains('2013')])\n",
    "t_2014 = len(df[df['公开(公告)日'].str.contains('2014')])\n",
    "t_2015 = len(df[df['公开(公告)日'].str.contains('2015')])\n",
    "t_2016 = len(df[df['公开(公告)日'].str.contains('2016')])\n",
    "t_2017 = len(df[df['公开(公告)日'].str.contains('2017')])\n",
    "t_2018 = len(df[df['公开(公告)日'].str.contains('2018')])\n",
    "t_2019 = len(df[df['公开(公告)日'].str.contains('2019')])\n",
    "t_2020 = len(df[df['公开(公告)日'].str.contains('2020')])\n",
    "t_2021 = len(df[df['公开(公告)日'].str.contains('2021')])\n",
    "t_2022 = len(df[df['公开(公告)日'].str.contains('2022')])\n",
    "t_2023 = len(df[df['公开(公告)日'].str.contains('2023')])\n",
    "print(t_2013, t_2014, t_2015, t_2016, t_2017, t_2018, t_2019, t_2020, \n",
    "      t_2021, t_2021, t_2022, t_2023)\n",
    "\n",
    "# 生成相应的柱状图\n",
    "x_data = ['2013', '2014', '2015', '2016', '2017', '2018', '2019', '2020', \n",
    "          '2021', '2022', '2023']\n",
    "y_data = [t_2013, t_2014, t_2015, t_2016, t_2017, t_2018, t_2019, t_2020, \n",
    "          t_2021, t_2021, t_2022, t_2023]\n",
    "print(t_2013, t_2014, t_2015, t_2016, t_2017, t_2018, t_2019, t_2020, \n",
    "      t_2021, t_2021, t_2022, t_2023)\n",
    "\n",
    "\n",
    "plt.rcParams['font.sans-serif'] = ['SimHei']\n",
    "plt.rcParams['axes.unicode_minus'] = False\n",
    "\n",
    "print(len(x_data))\n",
    "# 画图\n",
    "for i in range(len(x_data)):\n",
    "    \n",
    "    plt.bar(x_data[i], y_data[i],width = 0.5, color=\"#87CEFA\")\n",
    "\n",
    "#设置图片名称\n",
    "plt.title(\"专利趋势分析\")\n",
    "#设置x轴标签名\n",
    "plt.xlabel(\"专利公告年份\")\n",
    "#设置y轴标签名\n",
    "plt.ylabel(\"专利数量\")\n",
    "\n",
    "fig = plt.gcf()\n",
    "fig.set_size_inches(8, 6)\n",
    "\n",
    "plt.savefig(r'D:\\jupyter-notebook\\ProjectChuang/time_bar_chart.jpg')\n",
    "\n",
    "#显示\n",
    "plt.show()\n"
